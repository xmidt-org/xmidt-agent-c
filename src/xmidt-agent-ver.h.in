#ifndef _XMIDT_AGENT_VERSION_H_
#define _XMIDT_AGENT_VERSION_H_ 1

#define XMIDT_AGENT_VERSION          "@xmidt-agent_VERSION@"

#define XMIDT_AGENT_VERSION_MAJOR    @xmidt-agent_VERSION_MAJOR@
#define XMIDT_AGENT_VERSION_MINOR    @xmidt-agent_VERSION_MINOR@
#define XMIDT_AGENT_VERSION_PATCH    @xmidt-agent_VERSION_PATCH@

/* This is the numeric version of the xmidt-agent version number, meant for easier
 * parsing and comparisons by programs.  The XMIDT_AGENT_VERSION_NUM define will
 * always follow this syntax:
 *
 *      0xAABBCC
 *
 * Where AA, BB, and CC are each major version, minor version, patch in hex
 * using 8 bits each.
 *
 * It should be safe to compare two different versions with the greater number
 * being the newer version.
 */
#define XMIDT_AGENT_VERSION_NUM      @xmidt-agent_BITWISE_VERSION@

#endif
